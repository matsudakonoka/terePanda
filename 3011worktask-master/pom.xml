            <classifier>dto</classifier>
        </dependency>
        <dependency>
            <groupId>com.cnpc.epai</groupId>
            <artifactId>0011common-cloud</artifactId>
            <version>${base.version}</version>
        </dependency>

        <dependency>
            <groupId>org.springframework.cloud</groupId>
