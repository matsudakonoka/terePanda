        <dependency>
            <groupId>cn.hutool</groupId>
            <artifactId>hutool-all</artifactId>
            <version>5.5.2</version>
        </dependency>
        <dependency>
            <groupId>com.richfit.epai</groupId>
            <artifactId>060ASI-Base</artifactId>
            <version>2.2.6-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.richfit.epai</groupId>
            <artifactId>064A6Factory</artifactId>
            <version>2.2.6-SNAPSHOT</version>
            <exclusions>
                <exclusion>
                    <artifactId>060ASI-Base</artifactId>
                    <groupId>com.richfit.epai</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <artifactId>jackson-dataformat-xml</artifactId>
            <groupId>com.fasterxml.jackson.dataformat</groupId>
